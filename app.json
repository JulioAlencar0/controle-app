{
  "expo": {
    "name": "",
    "slug": "controle-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.svg",
    "scheme": "controleapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "resizeMode": "cover",
        "backgroundColor": "#000000"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000"
       
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
       [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/icon.svg",
          "dark": {
            "image": "./assets/icon.svg",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
